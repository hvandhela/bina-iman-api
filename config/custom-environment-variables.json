{
    "env": "NODE_ENV",
    "app": {
        "name": "APP_NAME",
        "url": "APP_URL",
        "port": "APP_PORT",
        "timezone": "APP_TIMEOZNE",
        "notif": "APP_NOTIF"
    },
    "db": {
        "mongoUrl": "MONGO_URL"
    },
    "filesystem": {
        "driver": "FS_DRIVER",
        "key": "FS_KEY",
        "secret": "FS_SECRET",
        "region": "FS_REGION",
        "endpoint": "FS_ENDPOINT",
        "bucket": "FS_BUCKET",
        "port": "FS_PORT"
    },
    "logstash": {
        "port": "LOGSTASH_PORT",
        "host": "LOGSTASH_HOST"
    },
    "minio": {
        "endpoint": "MINIO_ENDPOINT",
        "bucket": "MINIO_BUCKET",
        "path": "MINIO_PATH",
        "ssl": "MINIO_SSL",
        "port": "MINIO_PORT",
        "accesskey": "MINIO_ACCESSKEY",
        "secretkey": "MINIO_SECRETKEY",
        "defaultAvatarPath": "MINIO_DEFAULT_AVATAR_PATH"
    },
    "partnerManagementService": {
        "endpoint": "PARTNER_MANAGEMENT_SERVICE_ENDPOINT"
    },
    "recaptcha": {
        "serverKey": "RECAPTCHA_SERVER_KEY",
        "enable": "RECAPTCHA_ENABLE"
    },
    "apm": {
        "url": "APM_URL",
        "on": "APM_ON"
    }
}